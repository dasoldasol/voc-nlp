## 건축물용도 합치기 
def enrich_result_with_use_type(row, code_df):
    result = row.get("result_final")
    
    if not isinstance(result, dict):
        return result  # 변경 없이 반환

    main_purps_cd = result.get("mainPurpsCd")
    if not main_purps_cd:
        return result

    matched = code_df[code_df["use_cd"] == main_purps_cd]

    if not matched.empty:
        result["building_use_type_id"] = matched.iloc[0]["building_use_type_id"]
        result["building_use_type_name"] = matched.iloc[0]["name"]
    
    return result


