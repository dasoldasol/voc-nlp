import requests
from typing import Tuple, Optional

def get_address_info(address: str, api_key: str) -> Tuple[Optional[str], Optional[str], Optional[str]]:
    url = "https://dapi.kakao.com/v2/local/search/address.json"
    headers = {
        "Authorization": f"KakaoAK {api_key}"
    }
    params = {
        "query": address
    }

    response = requests.get(url, headers=headers, params=params)
    response.raise_for_status()

    data = response.json()
    documents = data.get("documents", [])
    
    if documents and "address" in documents[0]:
        addr = documents[0]["address"]
        b_code = addr.get("b_code")
        main_no = addr.get("main_address_no")
        sub_no = addr.get("sub_address_no")
        return b_code, main_no, sub_no
    else:
        return None, None, None


building_df[['b_code', 'main_address_no', 'sub_address_no']] = building_df['address'].apply(
    lambda addr: pd.Series(get_address_info(addr, KAKAO_API_KEY))
)


def extract_address_info(bun, ji, b_code):
    bunji_bun = str(bun).zfill(4) if bun not in [None, ''] else "0000"
    bunji_ji = str(ji).zfill(4) if ji not in [None, ''] else "0000"
    
