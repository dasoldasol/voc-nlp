from datetime import datetime
failed_ids = []

def query_building_detail_from_row(row, service_key, delay=0.3):
    import requests
    import time

    def fetch_br_basis_ouln_info(sigunguCd, bjdongCd, bun, ji):
        endpoint = "https://apis.data.go.kr/1613000/BldRgstHubService/getBrBasisOulnInfo"
        params = {
            "serviceKey": service_key,
            "sigunguCd": sigunguCd,
            "bjdongCd": bjdongCd[5:],
            "bun": bun.zfill(4),
            "ji": ji.zfill(4),
            "numOfRows": 100,
            "pageNo": 1,
            "_type": "json"
        }
        response = requests.get(endpoint, params=params)
        response.raise_for_status()
        data = response.json()
        item = data.get("response", {}).get("body", {}).get("items", {}).get("item", {})
        return item[0] if isinstance(item, list) else item

    def fetch_title_info(endpoint, sigunguCd, bjdongCd, bun, ji):
        params = {
            "serviceKey": service_key,
            "sigunguCd": sigunguCd,
            "bjdongCd": bjdongCd[5:],
            "bun": bun.zfill(4),
            "ji": ji.zfill(4),
            "numOfRows": 100,
            "pageNo": 1,
            "_type": "json"
        }
        response = requests.get(endpoint, params=params)
        response.raise_for_status()
        data = response.json()
        item = data.get("response", {}).get("body", {}).get("items", {}).get("item", {})
        return item

    try:
        sigunguCd = row["sgg_code"]
        bjdongCd = row["b_code"]
        bun = row["main_address_no"]
        ji = row["sub_address_no"]

        result_summary = {}

        # 1. 기본개요 조회
        basis_info = fetch_br_basis_ouln_info(sigunguCd, bjdongCd, bun, ji)
        bldNm = basis_info.get("bldNm")
        result_summary["bldNm"] = bldNm
        result_summary["regstrGbCdNm"] = basis_info.get("regstrGbCdNm")
        regstrGbCd = basis_info.get("regstrGbCd")
        mgmUpBldrgstPk = basis_info.get("mgmUpBldrgstPk")

        time.sleep(delay)

        # 표제부 미리 조회 (집합/단일 모두 대비)
        title_endpoint = "https://apis.data.go.kr/1613000/BldRgstHubService/getBrTitleInfo"
        title_items = fetch_title_info(title_endpoint, sigunguCd, bjdongCd, bun, ji)
        title_target = None
        if isinstance(title_items, list) and title_items:
            main = [i for i in title_items if i.get("mainAtchGbCdNm") == "주건축물"]
            title_target = max(main, key=lambda x: float(x.get("totArea", 0))) if main else max(title_items, key=lambda x: float(x.get("totArea", 0)))

        # 집합 건축물 처리
        if regstrGbCd == "2":
            recap_endpoint = "https://apis.data.go.kr/1613000/BldRgstHubService/getBrRecapTitleInfo"
            recap_items = fetch_title_info(recap_endpoint, sigunguCd, bjdongCd, bun, ji)
            recap = recap_items[0] if isinstance(recap_items, list) and recap_items else {}

            # recap에서 우선 채우고, 비어 있으면 title_target에서 보완
            def get_field(key):
                return recap.get(key) or (title_target.get(key) if title_target else None)

            result_summary.update({
                "platArea": get_field("platArea"),
                "archArea": get_field("archArea"),
                "totArea": get_field("totArea"),
                "bcRat": get_field("bcRat"),
                "vlRat": get_field("vlRat"),
                "mainPurpsCd": get_field("mainPurpsCd"),
                "stcnsDay": get_field("stcnsDay"),
            })

            # title_target에서 나머지 보완
            if title_target:
                result_summary.update({
                    "heit": title_target.get("heit"),
                    "grndFlrCnt": title_target.get("grndFlrCnt"),
                    "ugrndFlrCnt": title_target.get("ugrndFlrCnt"),
                    "mgmBldrgstPk": title_target.get("mgmBldrgstPk"),
                })
                if not result_summary.get("bldNm") or str(result_summary["bldNm"]).strip() == "":
                    result_summary["bldNm"] = title_target.get("bldNm")

        else:
            # 단일 건축물 처리
            if title_target:
                result_summary.update({
                    "platArea": title_target.get("platArea"),
                    "archArea": title_target.get("archArea"),
                    "totArea": title_target.get("totArea"),
                    "bcRat": title_target.get("bcRat"),
                    "vlRat": title_target.get("vlRat"),
                    "mainPurpsCd": title_target.get("mainPurpsCd"),
                    "stcnsDay": title_target.get("stcnsDay"),
                    "heit": title_target.get("heit"),
                    "grndFlrCnt": title_target.get("grndFlrCnt"),
                    "ugrndFlrCnt": title_target.get("ugrndFlrCnt"),
                    "mgmBldrgstPk": title_target.get("mgmBldrgstPk"),
                })
                if not result_summary.get("bldNm") or str(result_summary["bldNm"]).strip() == "":
                    result_summary["bldNm"] = title_target.get("bldNm")=

        crtn_candidates = []

        basis_crtn = basis_info.get("crtnDay")
        if basis_crtn and basis_crtn.strip():
            crtn_candidates.append(basis_crtn.strip())

        recap_crtn = recap.get("crtnDay") if 'recap' in locals() and recap else None
        if recap_crtn and recap_crtn.strip():
            crtn_candidates.append(recap_crtn.strip())

        title_crtn = title_target.get("crtnDay") if title_target else None
        if title_crtn and title_crtn.strip():
            crtn_candidates.append(title_crtn.strip())

        # 가장 최신 crtnDay 선택
        if crtn_candidates:
            try:
                latest_crtn = max(crtn_candidates, key=lambda x: datetime.strptime(x, "%Y%m%d"))
                result_summary["crtnDay"] = latest_crtn
            except ValueError:
                result_summary["crtnDay"] = None
        else:
            result_summary["crtnDay"] = None


        
        

        return result_summary

    except Exception as e:
        print(f"[Error] id={row.get('id')} name={row.get('name')} → {e}")
        failed_ids.append(row.get("id"))
        return None


