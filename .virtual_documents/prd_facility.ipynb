import pandas as pd
import numpy as np


FACILITY_CATEGORY_ID = 1163
WRITER_ID = 4
LAST_MODIFIER_ID = 4
DEFAULT_FACILITY_MASTER_ID = 1071  # model_name 없는 경우 사용할 기본값 


def normalize(s):
    return re.sub(r'[^A-Za-z0-9가-힣]', '', str(s))


def get_or_blank(value, transform_func=None):
    if pd.isna(value):
        return ''
    return transform_func(value) if transform_func else value


# NaN -> None 변환 함수
def get_or_none(value, transform_func=None):
    if pd.isna(value):
        return None
    return transform_func(value) if transform_func else value


try:

    conn = psycopg2.connect(
        host="hdcl-csp-rds-aurora-cluster.cluster-cody3sv8qrid.ap-northeast-2.rds.amazonaws.com",
        dbname="csp_prd",
        user="csp_admin",
        password="hdci12#$",
        port=5432
    )
    cur = conn.cursor()
    
    df["facility_master_id"] = None

    # facility_master 테이블에서 model_name 포함 조회
    cur.execute("SELECT id, name, model_name FROM facility_master")
    master_rows = cur.fetchall()
    master_df = pd.DataFrame(master_rows, columns=["id", "name", "model_name"])
    master_df["normalized_model_name"] = master_df["model_name"].apply(lambda x: normalize(x) if pd.notna(x) else '')

    for idx, row in df.iterrows():
        model_name = row.get("model_name")
        if pd.isna(model_name) or str(model_name).strip() == '':
