import re 


## 날짜 
def parse_partial_date(value):
    if pd.isna(value):
        return None
    try:
        value_str = str(value).strip()

        # 연도만 들어온 경우 ('2024' 등)
        if re.fullmatch(r'\d{4}', value_str):
            return pd.to_datetime(f"{value_str}-01-01")

        # 가능한 형식 순서대로 시도
        for fmt in ('%Y.%m.%d', '%Y.%m', '%Y-%m-%d'):
            try:
                return pd.to_datetime(value_str, format=fmt)
            except ValueError:
                continue

        # 마지막 수단: 자동 인식
        return pd.to_datetime(value_str, errors='coerce')
    except Exception:
        return None


