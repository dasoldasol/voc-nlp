            df.at[idx, "facility_master_id"] = DEFAULT_FACILITY_MASTER_ID
            continue

        normalized_model_name = normalize(model_name)

        # model_name 기준 중복 체크
        matched = master_df[master_df["normalized_model_name"] == normalized_model_name]
        if not matched.empty:
            df.at[idx, "facility_master_id"] = matched.iloc[0]["id"]
            continue

        # use가 없으면 name으로 대체
        use_value = row.get("use")
        if pd.isna(use_value) or str(use_value).strip() == "":
            use_value = row.get("name")

        insert_sql = """
            INSERT INTO facility_master (
                facility_category_id, name, use, making_company, making_company_phone,
                model_name, electricity_consumption, capacity, fuel_type,
                writer_id, write_date, last_modifier_id, last_modify_date
            )
            VALUES (%s, %s, %s, %s, %s, %s, %s, %s, %s, %s, %s, %s, %s)
