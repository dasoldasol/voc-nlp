def clean(text):
    if pd.isna(text):
        return ''
    text = str(text).strip()
    text = text.replace('\xa0', '').replace('\u200b', '').replace('\ufeff', '')
    text = re.sub(r'\s+', '', text)          # 공백 제거
    text = re.sub(r'[^가-힣a-zA-Z0-9]', '', text)  # 괄호 등 특수문자 제거
    return text
    
