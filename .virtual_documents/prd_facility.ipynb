            get_or_blank(row.get("name")),
            get_or_blank(use_value),
            get_or_none(row.get("making_company")),
            get_or_none(row.get("making_company_phone")),
            get_or_blank(model_name),
            get_or_none(row.get("electricity_consumption")),
            get_or_none(row.get("capacity")),
            get_or_none(row.get("fuel_type")),
            WRITER_ID,
            datetime.now(),
            LAST_MODIFIER_ID,
            datetime.now()
        )
        try:
            cur.execute(insert_sql, values)
            new_id = cur.fetchone()[0]
            conn.commit()
            df.at[idx, "facility_master_id"] = new_id

            # INSERT한 모델도 master_df에 추가 (중복 방지)
            new_row = {
                "id": new_id,
                "name": row.get("name"),
                "model_name": model_name,
                "normalized_model_name": normalized_model_name
            }
            master_df = pd.concat([master_df, pd.DataFrame([new_row])], ignore_index=True)

        except Exception as e:
            conn.rollback()
            print(f"[INSERT ERROR] row {idx}: {e}")

except Exception as e:
